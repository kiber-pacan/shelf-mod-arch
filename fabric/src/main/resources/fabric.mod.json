{
  "schemaVersion": 1,
  "id": "${modid}",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": [
    "${authors}"
  ],
  "license": "${license}",
  "icon": "assets/shelfmod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.akciater.fabric.ShelfModFabric"
    ],
    "client": [
      "com.akciater.fabric.client.ShelfModFabricClient"
    ]
  },
  "mixins": [
    "shelfmod.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "${minecraft_version}",
    "java": ">=${java}",
    "architectury": "${architectury_api_version}",
    "fabric-api": "*"
  }
}
